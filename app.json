{
    "expo": {
        "name": "Tuyển Dụng",
        "slug": "mobi-tuyen-dung",
        "version": "1.0.0",
        "orientation": "portrait",
        "sdkVersion": "51.0.0",
        "icon": "./assets/images/icon-logo.png",
        "scheme": "myapp",
        "userInterfaceStyle": "automatic",
        "splash": {
            "image": "./assets/images/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "y",
            "infoPlist": {
                "ITSAppUsesNonExemptEncryption": false
            }
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/icon-logo.png",
                "backgroundColor": "#6A0DAD"
            },
            "permissions": [
                "READ_EXTERNAL_STORAGE",
                "WRITE_EXTERNAL_STORAGE"
            ],
            "package": "com.nganhkiet2003.mobituyendung"
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./assets/images/favicon.png"
        },
        "plugins": [
            "expo-router",
            "expo-secure-store"
        ],
        "experiments": {
            "typedRoutes": true
        },
        "extra": {
            "apiUrl": "https://tuyendungpkl.xyz/api/v1",
            "router": {
                "origin": false
            },
            "eas": {
                "projectId": "3a9f66b8-243f-414d-a361-0650be128bcf"
            }
        },
        "runtimeVersion": {
            "policy": "appVersion"
        },
        "updates": {
            "url": "https://u.expo.dev/3a9f66b8-243f-414d-a361-0650be128bcf"
        }
    }
}
